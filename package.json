{
  "name": "kew",
  "version": "0.4.0-beta.2",
  "description": "A typed task queue with a simple API",
  "main": "dist/index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "pnpm run --recursive build",
    "prettier": "pnpm run --recursive prettier && prettier --write .",
    "test": "pnpm run --recursive test && prettier --check .",
    "test:prettier": "pnpm run --recursive test:prettier && prettier --check .",
    "test:tsc": "pnpm run --recursive test:tsc",
    "run-example": "pnpm start --filter '@exogee/kew-example'",
    "pub:dry": "pnpm publish --recursive --access public --dry-run",
    "pub": "pnpm publish --recursive --access public"
  },
  "devDependencies": {
    "typescript": "4.3.5",
    "esbuild": "0.15.13",
    "esbuild-register": "3.4.1",
    "prettier": "2.8.0"
  }
}
